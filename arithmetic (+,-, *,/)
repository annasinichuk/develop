#include <stdio.h>
int main() {
    int numeral, numeral1;
    char symbol;
    if (scanf("%d %d%c", &numeral, &numeral1, &symbol) != 3 || symbol != '\n') {
        printf("n/a");
    } else {
    printf("%d ", numeral + numeral1);
    printf("%d ", numeral - numeral1);
    printf("%d ",numeral * numeral1);
    if (numeral1 == 0) {
        printf("n/a");
    } else {
        printf("%d", numeral / numeral1);
    }
    }
    return 0;
    }
