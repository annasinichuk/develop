#include <stdio.h>
#include <math.h>

int main () {
double R, V, m;
char chr;
if (scanf("%lf%c", &R, &chr) == 2 && 0 < R && chr == 10) {
V = ((R*4/3) * R * M_PI *R);
m = V * 21500;
printf("%.0lf", m);
} else {
printf("n/a");
}
return 0;
}
// Необходимо вычислить массу сферы из платины
Радиус R (в метрах) дается на вход, равен 1 (10 во втором случае)
Плотность платины 21500
pi = M_PI (константа) разрешено использовать библиотеку <math.h>
V = (4/3)*pi*R^3
P = m/V
